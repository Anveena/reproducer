{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ServiceOfOpenSSLQUICBugReproducer",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/openSSL/service/main.go",
      "cwd": "${workspaceFolder}/openSSL/service",
      "output": "${workspaceFolder}/openSSL/service/build/service"
    },
    {
      "name": "ClientOfOpenSSLQUICBugReproducer",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/openSSL/client/build/client",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "SmartBuildClientOfOpenSSLQUICBugReproducer",
      "miDebuggerPath": "gdb"
    }
  ]
